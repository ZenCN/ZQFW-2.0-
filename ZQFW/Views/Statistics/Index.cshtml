        <div id="map" class="slide_page">
            <iframe height="100%" width="100%" style=" background-color:White;" frameborder="0" src="/Statistics/Map"></iframe>
        </div>
        <div id="analysis" class="slide_page" style="display: none;">
        </div>
        <div id="assessment" class="slide_page" style="display: none;">
        </div>
    </div>
    <%--模块阴影效果--%>
    <div class="main_shadows">
        <div class="leftshadow">
        </div>
        <div class="rightshadow">
        </div>
        <div class="leftbottomshadow">
